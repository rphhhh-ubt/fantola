# Test Environment Variables
# Copy this file to .env.test and update with your test configuration

# Node Environment
NODE_ENV=test

# Database (Test)
POSTGRES_TEST_DB=monorepo_test
POSTGRES_TEST_USER=postgres
POSTGRES_TEST_PASSWORD=postgres
POSTGRES_TEST_PORT=5433
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/monorepo_test

# Redis (Test)
REDIS_TEST_PORT=6380
REDIS_URL=redis://localhost:6380

# Logging
LOG_LEVEL=error

# API
API_PORT=3001

# Test-specific settings
TEST_TIMEOUT=10000
